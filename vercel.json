{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "crons": [
    {
      "path": "/api/auto_archive",
      "schedule": "0 1 * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/simple_test",
      "dest": "/api/simple_test.py"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/search",
      "dest": "/api/search.py"
    },
    {
      "src": "/api/download",
      "dest": "/api/download.py"
    },
    {
      "src": "/api/archive",
      "dest": "/api/archive.py"
    },
    {
      "src": "/api/optimize_keywords",
      "dest": "/api/optimize_keywords.py"
    },
    {
      "src": "/api/manage_categories",
      "dest": "/api/manage_categories.py"
    },
    {
      "src": "/api/auto_archive",
      "dest": "/api/auto_archive.py"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/register",
      "dest": "/api/register.py"
    },
    {
      "src": "/api/upgrade",
      "dest": "/api/upgrade.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ]
}
