{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/search",
      "dest": "/api/search.py"
    },
    {
      "src": "/api/download",
      "dest": "/api/download.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ]
}
