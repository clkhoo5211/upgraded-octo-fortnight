{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/search",
      "dest": "/api/search.py"
    },
    {
      "src": "/api/download",
      "dest": "/api/download.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ]
}
